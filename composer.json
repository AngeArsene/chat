{
    "name": "angearsene/chat",
    "description": "Laravel chat package",
    "type": "library",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.38"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "orchestra/testbench": "^10.6"
    },
    "scripts": {
        "tests": "vendor/bin/phpunit --coverage-text"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AngeArsene\\Chat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AngeArsene\\Chat\\Tests\\": "tests/",
            "AngeArsene\\Chat\\Tests\\Models\\": "tests/helpers/models/",
            "AngeArsene\\Chat\\Tests\\Concerns\\": "tests/helpers/concerns/",
            "AngeArsene\\Chat\\Tests\\Database\\Factories\\": "tests/helpers/database/factories/"
        }
    },
    "authors": [
        {
            "name": "Ange Arsene",
            "email": "nkenmandenga@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
